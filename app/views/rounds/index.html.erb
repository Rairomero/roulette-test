<p id="notice"><%= notice %></p>

<h1>Rounds</h1>

<table>
  <thead>
    <tr>
      <th>Round</th>
      <th>Result</th>
      <th>Pot</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rounds.each do |round| %>
      <tr>
        <td><%= round.id %></td>
        <td><%= round.result %></td>
        <td><%= round.pot %></td>
        <td><%= link_to 'Show', round %></td>
      </tr>
    <% end %>
  </tbody>
</table>
  <%= paginate @rounds %>

<%= link_to 'Players', players_path %>
